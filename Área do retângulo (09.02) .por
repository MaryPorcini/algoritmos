programa {
  funcao inicio() {
    //Área de um Retângulo
    //Declaração de Variáveis
    inteiro x1, x2
    real area

    //Entrada
    escreva ("Qual é a altura do terreno?: ")
    leia (x1)
    escreva ("Qual é a base do terreno?: ")
    leia (x2)

    //Processamento
    area = x1 * x2

    //Saída
    escreva ("A área do seu terreno é:", area, " metros cubicos") 

  }
}
