programa {
  funcao inicio() {
    //Temperatura
    //Declaração de variáveis
    real celsius, fahrenheit

    //Entrada
    escreva ("Qual é a temperatura? :")
    leia (celsius)

    //Processamento
    fahrenheit = (celsius * 1.8) + 32

    //Saída
    escreva ("A temperatura é: ", fahrenheit, " em graus fahrenheit")
    
  }
}
