programa {
  funcao inicio() {

    //Valor/Quantidade
    //Declaração de Variáveis
    real valor, q, total

    //Entrada
    escreva ("Digite o valor do produto: ")
    leia(valor)
    escreva ("Digite a quantidade do produto: ")
    leia(q)

    //Processamento
    total = valor * q

    //Saída
    escreva ("O valor total a ser pago pelo produto é: ", total)

    
  }
}
